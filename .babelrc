{
    "presets": [
        [
            "@babel/preset-env",
            {
              "targets": { "ie": "10" },
              "useBuiltIns": "usage",
              "corejs": 3,
              "debug": true
            }
        ],
        "@babel/preset-react",
    ],
    "plugins": [
        "@babel/proposal-class-properties",
        "@babel/plugin-transform-runtime",
        "@babel/plugin-transform-regenerator",
        ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": "css" }]
    ]
}